	.DB	0
	.DB	0
	.DB	"K"
	.DB	"I"
; Display DCB
	.DB	7
	.DW	DSPDRV		; DISPLAY driver address
	.DW	vidmem		; Cursor Position
	.DB	0
	.DB	"D"
	.DB	"O"
