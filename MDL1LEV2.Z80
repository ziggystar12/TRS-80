FFXLV1:	call	m,FINDIV	;11d0
	jp	m,FFXLV1	;11d3
	pop	bc		;11d6
	pop	af		;11d7
	push	bc		;11d8
	push	af		;11d9
	jp	m,FFXLV3	;11da
	xor	a		;11dd
FFXLV3:	cpl			;11de
	inc	a		;11df
	add	a,b		;11e0
	inc	a		;11e1
	add	a,d		;11e2
	ld	b,a		;11e3
	ld	c,$00		;11e4
