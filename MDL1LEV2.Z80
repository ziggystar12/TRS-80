; ----------------------------------------------------------------------
AUTO:	ld	de,000AH	;2008
	push	de		;200b
	jr	z,SNGAUT	;200c
	call	LINSPC		;200e
	ex	de,hl		;2011
	ex	(sp),hl		;2012
; At this point, DE points to the current character in the BASIC line
; being processed, “10” is in HL, and the initial number is on the STACK.
	jr	z,SNGAU1	;2013
	ex	de,hl		;2015
	rst	08h		;2016
	.DB	','		;2017
	ex	de,hl		;2018
	ld	hl,(AUTINC)	;2019
	ex	de,hl		;201c
	jr	z,SNGAUT	;201d
	call	LINGET		;201f
	jp	nz,SNERR	;2022
SNGAUT:	ex	de,hl		;2025
SNGAU1:	ld	a,h		;2026
	or	l		;2027
	jp	z,FCERR		;2028
	ld	(AUTINC),hl	;202b
	ld	(AUTFLG),a	;202e
	pop	hl		;2031
	ld	(AUTLIN),hl	;2032
	pop	bc		;2035
	jp	MAIN		;2036
