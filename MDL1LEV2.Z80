	call	FOUTED		;105b
	ld	(hl),$30	;105e
	call	z,INXHRT	;1060
	call	FOUTCV		;1063
FOFRS2:	dec	hl		;1066
	ld	a,(hl)		;1067
	cp	'0'		;1068
	jr	z,FOFRS2	;106a
	cp	'.'		;106c
