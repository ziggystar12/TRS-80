	pop	de		;10ac
	ld	a,d		;10ad
	sub	$05		;10ae
	call	p,FOTZER	;10b0
	call	FOUTCI		;10b3
FOUTTD:	ld	a,e		;10b6
	or	a		;10b7
	call	z,DCXHRT	;10b8
	dec	a		;10bb
	call	p,FOTZER	;10bc
; ----------------------------------------------------------------------
; 10BF – MATH ROUTINE – “FOUTTS”
;   This routine will finish up the printing of a fixed format number.
; ----------------------------------------------------------------------
FOUTTS:	push	hl		;10bf
	call	FOUTZS		;10c0
	pop	hl		;10c3
	jr	z,FFXIX1	;10c4
