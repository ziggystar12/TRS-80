	pop	af		;11e9
	call	p,FOTZNC	;11ea
	pop	bc		;11ed
	pop	af		;11ee
	call	z,DCXHRT	;11ef
	pop	af		;11f2
	jr	c,FFXLV2	;11f3
	add	a,e		;11f5
