	call	FOUTDP		;1183
	ld	c,a		;1186
	xor	a		;1187
	sub	d		;1188
	sub	e		;1189
	call	FOTZER		;118a
	push	bc		;118d
	ld	b,a		;118e
	ld	c,a		;118f
FFXXV6:	call	FOUTCV		;1190
	pop	bc		;1193
	or	c		;1194
