	.DB	6		; 4025
	.DW	PRTDRV		; driver address
	.DB	43h		; lines per page
	.DB	0
	.DB	0
	.DB	"P"
	.DB	"R"
	jp	$5000		;402d
	rst	00h		;4030
	nop
	nop
	ld	a, 0		;4033
	ret
