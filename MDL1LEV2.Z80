FOUBE1:	inc	hl		;10cd
FOUBE5:	ld	a,(TEMP2)	;10ce
	sub	l		;10d1
	sub	d		;10d2
	ret	z		;10d3
	ld	a,(hl)		;10d4
	cp	SPACE		;10d5
	jr	z,FOUBE1	;10d7
