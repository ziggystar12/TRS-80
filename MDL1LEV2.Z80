	ld	(prtstat),a	;05ad
	djnz	PRTFF		;05b0
	jr	PRTOP		;05b2
PRTIT:	push	af		;05b4
PRTIT2:	call	PRTSA		;05b5
	jr	nz,PRTIT2	;05b8
	pop	af		;05ba
	ld	(prtstat),a	;05bb
	cp	CRET		;05be
	ret	nz		;05c0
	inc	(ix+$04)	;05c1
	ld	a,(ix+$04)	;05c4
	cp	(ix+$03)	;05c7
	ld	a,c		;05ca
	ret	nz		;05cb
PRTOP:	ld	(ix+$04),0	;05cc
	ret			;05d0
