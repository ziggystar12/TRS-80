; ----------------------------------------------------------------------
; 0573H – Part of the Display routine – “DSPEOL”
;   Erase to the end of the line
; ----------------------------------------------------------------------
DSPEOL:	push	hl		;0573
	ld	d,h		;0574
	ld	a,l		;0575
	or	$3F		;0576
	ld	e,a		;0578
	inc	de		;0579
	jr	DSPERA		;057a
; ----------------------------------------------------------------------
; 057CH – Part of the Display routine – “DSPEOF”
;   Erase to the end of the frame. clear the video screen from (including)
