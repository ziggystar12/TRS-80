	sub	b		;116c
	pop	bc		;116d
	ld	e,a		;116e
	add	a,d		;116f
	ld	a,b		;1170
	jp	m,FFXXV3	;1171
	sub	d		;1174
	sub	e		;1175
	call	p,FOTZER	;1176
	push	bc		;1179
	call	FOUTCD		;117a
	jr	FFXXV6		;117d
; ----------------------------------------------------------------------
; 117F – MATH ROUTINE – “FFXXV3”
