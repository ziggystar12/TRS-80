; ----------------------------------------------------------------------
; 1085 – MATH ROUTINE – “FOUCE1” and “FOUCE2”
; ----------------------------------------------------------------------
FOUCE1:	ld	b,$2F		;1085
FOUCE2:	inc	b		;1087
