	ret	c		;0510
	cp	$0E		;0511
	jr	c,DSPCR		;0513
	jr	z,DSPCON	;0515
	cp	$0F		;0517
	jr	z,DSPCOF	;0519
	cp	$17		;051b
	jr	z,DSPETB	;051d
	cp	$18		;051f
	jr	z,DSPLFT	;0521
	cp	$19		;0523
	jr	z,DSPRHT	;0525
	cp	$1A		;0527
	jr	z,DSPDWN	;0529
	cp	$1B		;052b
	jr	z,DSPUP		;052d
	cp	$1C		;052f
	jr	z,DSPHOM	;0531
