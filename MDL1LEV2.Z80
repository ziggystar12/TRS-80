; ----------------------------------------------------------------------
; 069FH – Bootstrap from Diskette – "BOOT"
; ----------------------------------------------------------------------
BOOT:	ld	a,$01		;069f
	ld	(dskselect),a	;06a1
	ld	hl,diskstat
